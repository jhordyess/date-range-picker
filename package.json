{
  "name": "date-range-pricker",
  "description": "Date Range Picker is a React component to select a date range.",
  "version": "1.1.0",
  "author": "Jhordy Gavinchu <dev@jhordyess.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "reactjs",
    "taiwindcss",
    "daterangepicker",
    "luxon",
    "lodash"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write .",
    "predeploy": "yarn lint && yarn build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.2.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  }
}
